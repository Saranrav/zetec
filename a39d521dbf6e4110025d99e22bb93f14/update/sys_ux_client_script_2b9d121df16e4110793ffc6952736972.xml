        <script><![CDATA[/**
 * @param {params} params
 * @param {api} params.api
 * @param {any} params.event
 * @param {any} params.imports
 */
function handler({
    api,
    event,
    helpers,
    imports
}) {

    // saving the form values. When save draft is pressed, the status of the record is submitted
    api.data.gform.setValue({
        fieldName: 'state',
        value: 'submitted'
    });

    api.data.gform.save();

}]]></script>
